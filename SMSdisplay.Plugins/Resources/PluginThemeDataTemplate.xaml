<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:SMSdisplay.Plugins">

    <DataTemplate DataType="{x:Type local:PluginTheme}" x:Key="PluginThemeTemplate">
        <DataTemplate.Resources>
        </DataTemplate.Resources>
        <StackPanel Name="item" Orientation="Horizontal" Height="16">
            <Grid>
            </Grid>
            <StackPanel Margin="1,0,1,1" VerticalAlignment="Center">
                <TextBlock Text="{Binding Name}" FontWeight="Bold" />
            </StackPanel>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>